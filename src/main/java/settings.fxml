<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="FXMLController">
   <children>
      <VBox alignment="CENTER" layoutX="1.0" layoutY="1.0" prefHeight="600.0" prefWidth="600.0">
         <children>
            <CheckBox fx:id="cbpbe" mnemonicParsing="false" text="Password-Based Encryption">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </CheckBox>
            <Label text="Password">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
            </Label>
            <PasswordField fx:id="pfpbe" maxWidth="150.0" prefWidth="150.0">
               <VBox.margin>
                  <Insets bottom="10.0" />
               </VBox.margin>
            </PasswordField>
            <Label alignment="TOP_LEFT" text="Algorithm">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets left="5.0" right="5.0" top="5.0" />
               </VBox.margin></Label>
            <ComboBox fx:id="cbalgo" prefWidth="150.0" />
            <Label text="Keysize">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="keysize" prefWidth="150.0" />
            <Label alignment="TOP_LEFT" text="Mode">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="cbmode" prefWidth="150.0" />
            <Label alignment="TOP_LEFT" text="Padding">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
            </Label>
            <ComboBox fx:id="cbpad" prefWidth="150.0" />
            <CheckBox fx:id="chbhash" layoutX="213.0" layoutY="133.0" mnemonicParsing="false" text="HMAC">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </CheckBox>
            <Label alignment="TOP_LEFT" layoutX="286.0" layoutY="360.0" text="Hashing">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Label>
            <ComboBox fx:id="cbhash" layoutX="235.0" layoutY="391.0" prefWidth="150.0" />
            <Button fx:id="apply" mnemonicParsing="false" onAction="#handleApplyButtonAction" prefHeight="30.0" prefWidth="90.0" text="Apply">
               <font>
                  <Font name="Calibri Light" size="15.0" />
               </font>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </Button>
         </children>
         <padding>
            <Insets left="50.0" right="50.0" />
         </padding></VBox>
   </children>
</AnchorPane>
